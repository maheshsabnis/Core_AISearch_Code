{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AzureSettings": {
        "AzureAISearchEndPoint": "https://[AZURE-AI-SEARCH-SERVICE].search.windows.net",
        "AzureAISearchKey": "[AZURE-AI-SEARCH-PRIMARY-ADMIN-KEY]",
