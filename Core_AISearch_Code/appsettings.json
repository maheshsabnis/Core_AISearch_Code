        "AzureAISearchIndexerName": "shipping-indexer",
        "AzureAISearchDataSourceName": "shipping-datasource",
        "AzureSQLDbConnectionString": "[AZURE-SQL-DATABASE-CONNECTION-STRING]"
    }
}
